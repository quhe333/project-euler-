public class Q6 {
     public static void main(String[] args){
	     int a = 0 ;
	     int b = 0 ;
	     
	     
	     for( int i = 1;  i <= 100;  i = i + 1){
	    	 a = a + i*i ;
	    	 b = (b+i) ; 
	    	 
	    	 
	     }
	     
	     b= b*b ;
	     
	     System.out.println(b-a);
	     
	 
	 
 }
}
